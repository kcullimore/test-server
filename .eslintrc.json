{
    "extends": [
        "eslint:recommended",
        "plugin:import/errors",
        "prettier"
    ],
    "rules": {
        "prettier/prettier": 0
    },
    "parser": "babel-eslint",
    "plugins": ["import"],
    "parserOptions": {
        "ecmaVersion": 2019,
        "sourceType": "module"
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    }
}
